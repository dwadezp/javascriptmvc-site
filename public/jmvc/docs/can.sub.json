c({"name": "can.sub", "type": "function", "comment": "<p>Returns a string with {param} replaced values from data.</p>\n\n<pre><code>can.sub(\"foo {bar}\",{bar: \"far\"})\n//-&gt; \"foo far\"\n</code></pre>", "parents": ["can.util"], "params": {"s": {"order": 0, "description": "<p>The string to replace</p>", "name": "s", "type": "String", "optional": false}, "data": {"order": 1, "description": "<p>The data to be used to look for properties.  If it's an array, multiple\nobjects can be used.</p>", "name": "data", "type": "Object", "optional": false}, "remove": {"order": 2, "description": "<p>if a match is found, remove the property from the object</p>", "name": "remove", "type": "Boolean", "optional": true}}, "src": "can/util/string/string.js", "line": 144, "children": []})