steal('can/control', 'can/view/ejs', './views/init.ejs', function(){

/**
 * @class <%= name %>
 */
can.Control('<%= name %>',
/** @Static */
{
	defaults : {}
},
/** @Prototype */
{
	init : function(){
		this.element.html(can.view("//<%= path %>/<%= underscore %>/views/init.ejs",{
			message: "Hello World"
		}));
	}
})

});